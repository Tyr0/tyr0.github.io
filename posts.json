---
layout: null
---
{
  "posts": [
    {% for post in site.posts %}
      {
        "title": "{{ post.title | json }}",
        "url": "{{ post.url | json }}",
        "tags": ["{{ post.tags | join: '","' }}"],
        "created_at": {{ post.date | date: "%s" }},
        {% if post.modified %}
          "updated_at": {{ post.modified | date: "%s" }},
        {% else %}
          "updated_at": {{ post.date | date: "%s" }},
        {% endif %}
        "excerpt": "{{ post.excerpt | strip_html | strip_newlines | json }}"
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ],
  "meta": {
    "next": null
  }
}

